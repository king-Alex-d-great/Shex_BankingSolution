@using Microsoft.AspNetCore.Identity
@inject SignInManager<User>  signInManger
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ShEx BANK</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/css/site.css" rel="stylesheet" /> 
    <link href="~/css/custom.css" rel="stylesheet" />
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,700" rel="stylesheet" type="text/css">
    <link href="~/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/css/bootstrap-theme.min.css" rel="stylesheet" type="text/css">
    <link href="~/cssbootstrap-social.css" rel="stylesheet" type="text/css">
    <link href="~/css/templatemo_style.css" rel="stylesheet" type="text/css">
    <link href="~/css/templatemo-style-dashboard.css" rel="stylesheet" type="text/css">
   @*landing page*@

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">

    <link href="~/lib/LandingPage/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/lib/LandingPage/assets/css/fontawesome.css">
    <link rel="stylesheet" href="~/lib/LandingPage/assets/css/templatemo-space-dynamic.css">
    <link rel="stylesheet" href="~/lib/LandingPage/assets/css/animated.css">
    <link rel="stylesheet" href="~/lib/LandingPage/assets/css/owl.css">
</head>
<body>
   

    <div id="js-preloader" class="js-preloader">
        <div class="preloader-inner">
            <span class="dot"></span>
            <div class="dots">
                <span></span>
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <header class="header-area header-sticky wow slideInDown" data-wow-duration="0.75s" data-wow-delay="0s">
        <div class="container" style="width: 100%">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav">
                        <a asp-controller="Home" asp-action="Index" class="logo">
                            <h4>SHeX<span>Bank</span></h4>
                        </a>
                        <ul class="nav">
                            <partial name="_LoginPartial" />
                        </ul>

                            <a class='menu-trigger'>
                                <span>Menu</span>
                            </a>

</nav>
                </div>
            </div>
        </div>
    </header>


    @RenderBody()


    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 wow fadeIn" data-wow-duration="1s" data-wow-delay="0.25s">
                        <div class="container">
                            <p> © Copyright 2021 - ShEx BANK - <a asp-controller="Home" asp-action="Privacy">Privacy</a><br/>All Rights Reserved.</p>
                        </div>
                </div>
            </div>
        </div>
    </footer>


    <!-- Scripts -->
    <script src="~/lib/jquery.min.js"></script>
    <script src="~/lib/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @*landingpage*@
    <script src="~/lib/LandingPage/vendor/jquery/jquery.min.js"></script>
    <script src="~/lib/LandingPage/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/LandingPage/assets/js/owl-carousel.js"></script>
    <script src="~/lib/LandingPage/assets/js/animation.js"></script>
    <script src="~/lib/LandingPage/assets/js/imagesloaded.js"></script>
    <script src="~/lib/LandingPage/assets/js/templatemo-custom.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
